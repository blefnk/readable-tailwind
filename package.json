{
  "version": "0.0.0",
  "type": "module",
  "name": "eslint-plugin-readable-tailwind",
  "description": "auto-wraps tailwind classes after a certain width using template literals.",
  "license": "MIT",
  "author": "Roger Schönbächler",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "build": "vite build",
    "docs": "eslint-doc-generator",
    "eslint": "eslint --ext .ts,.tsx,.js,.jsx,.json,.jsonc,.yml,.md ./",
    "eslint:ci": "npm run eslint -- --max-warnings 0",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run markdownlint",
    "lint:ci": "npm run eslint:ci && npm run markdownlint:ci",
    "lint:fix": "npm run eslint:fix && npm run markdownlint:fix",
    "markdownlint": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "markdownlint:ci": "npm run markdownlint",
    "markdownlint:fix": "markdownlint-cli2-fix '**/*.md' '#node_modules'",
    "spellcheck": "cspell lint",
    "spellcheck:ci": "npm run spellcheck -- --no-progress",
    "test": "vitest -c ./vite.config.ts"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "devDependencies": {
    "@schoero/changelog-config": "^0.0.3",
    "@schoero/cspell-config": "^1.9.0",
    "@schoero/eslint-config": "^1.42.0",
    "@schoero/markdownlint-config": "^1.1.7",
    "@schoero/ts-config": "^0.3.8",
    "@schoero/vite-config": "^0.0.29",
    "@types/eslint": "^8.44.8",
    "@types/estree-jsx": "^1.0.3",
    "eslint": "^8.55.0",
    "eslint-doc-generator": "^1.6.1",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-no-bundle": "^3.0.0",
    "vitest": "^1.0.2"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "tailwind",
    "readable",
    "multiline",
    "multi",
    "line",
    "template",
    "literal",
    "template-literal",
    "template-literals",
    "tailwindcss",
    "tailwind-css",
    "tailwind-classes"
  ],
  "directories": {
    "lib": "lib"
  },
  "volta": {
    "node": "21.4.0"
  }
}
